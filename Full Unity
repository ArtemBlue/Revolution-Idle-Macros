Infinite()
WaitForSeconds(0.05)
Eternate()
WaitForSeconds(0.5)
Repeat (3, false)
	Dilate(true)
	WaitForSeconds(2.0)
	Dilate(false)
Repeat (20, false)
	EnterEC(0, 0, true)
	ExitEC()
Repeat (4, false)
	WaitForSeconds(0.05)
	LoadDT(0)
	LoadDT(1)
	LoadDT(2)
	LoadDT(3)
	Dilate(true)
	WaitForSeconds(2.0)
	Dilate(false)
	WaitForSeconds(2.0)
WaitUntil (EP == 0.00)
